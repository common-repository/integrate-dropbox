{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "integrate-dropbox/gallery",
  "version": "0.1.0",
  "title": "Gallery",
  "category": "integrate-dropbox",
  "icon": "format-gallery",
  "description": "Insert a mansory grid gallery with lightbox preview to display Dropbox image and videos.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "open": {
      "type": "boolean",
      "default": false
    },
    "isTrue": {
      "type": "boolean",
      "default": false
    },
    "editData": {
      "type": "object",
      "default": {
        "status": "on",
        "type": "Gallery",
        "allFolders": false,
        "folders": [],
        "privateFolders": false,
        "allowExtensions": null,
        "allowAllExtensions": false,
        "allowExceptExtensions": null,
        "allowNames": null,
        "allowAllNames": false,
        "allowExceptNames": null,
        "nameFilterOptions": [],
        "showFiles": true,
        "showFolders": true,
        "fileNumbers": "",
        "width": "100%",
        "height": "auto",
        "embedIframeWidth": "100%",
        "embedIframeHeight": "480px",
        "showFileName": false,
        "sort": {
          "sortBy": "Name",
          "sortDirection": "asc"
        },
        "view": "list",
        "layout": "Grid",
        "screenSize": "Desktop",
        "lazyLoad": true,
        "lazyloadnumber": 100,
        "maxFileUpload": null,
        "maxFileSize": "",
        "minFileSize": "",
        "enableFolderUpload": false,
        "openNewTab": true,
        "showUploadLabel": true,
        "uploadLabelText": "Upload Files",
        "allowEmbedPopout": true,
        "thumbnailCaption": true,
        "preview": true,
        "download": true,
        "displayFor": "everyone",
        "displayUsers": [
          "everyone"
        ],
        "displayEveryone": false,
        "displayExcept": [],
        "sliderPerPage": 4,
        "slideScreenSize": "",
        "slideGap": "",
        "rowheight": 200,
        "imgmargin": 10,
        "mobilecolumn": 2,
        "tabletcolumn": 3,
        "desktopcolumn": 4,
        "allowFileUploadUserRole": [
          {
            "value": "everyone",
            "label": "Everyone"
          }
        ],
        "accessDeniedMessage": null,
        "uploadConfirmationMessage": null,
        "isShowUploadLabelText": false,
        "thumbnailView": "Rounded",
        "folderView": "Title",
        "whoCanViewModule": "Everyone"
      }
    }
  },
  "textdomain": "integrate-dropbox",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}